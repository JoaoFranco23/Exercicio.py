create database Cadastro; 

use cadastro; 

create table cad(
Id INT AUTO_INCREMENT PRIMARY KEY,
Nome varchar(100) not null,
Idade integer,
Email varchar(50) not null,
Senha char(32) not null,
Cidade varchar(50),
Uf varchar(2), 
Cep int(8)
)engine=InnoDB default charset=latin1 auto_increment=253; 

